{
  "name": "applivery-store",
  "version": "1.0.7-3",
  "description": "Applivery Store",
  "main": "index.js",
  "scripts": {
    "dev": "poi -o",
    "build": "poi build",
    "lint": "eslint --ext .js,.vue src",
    "pretty": "prettier --write **/*.{js,json,scss,vue}",
    "build-deploy": "npm-run-all build deploy",
    "deploy": "firebase deploy --only hosting",
    "deploy:ci": "firebase deploy --only hosting --token ${FIREBASE_TOKEN} -m ${PKG_VERSION}",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "cypress run --record --key ${CYPRESS_KEY}",
    "serve": "serve -s dist",
    "test": "start-server-and-test serve http://localhost:5000 e2e:run",
    "test:ci": "start-server-and-test serve http://localhost:5000 e2e:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:applivery/applivery-store.git"
  },
  "author": "applivery",
  "license": "Apache 2.0",
  "private": true,
  "homepage": "https://github.com/applivery/applivery-store",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "firebase-tools": "^3.17.4",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "poi": "9.6.13",
    "poi-preset-bundle-report": "^2.0.2",
    "poi-preset-eslint": "^9.1.1",
    "poi-preset-sw-precache": "^1.0.0",
    "poi-preset-yaml": "^1.0.1",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "serve": "^6.5.0",
    "start-server-and-test": "^1.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cypress": "^2.0.3",
    "debug": "^3.1.0",
    "fuse.js": "^3.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "pretty-bytes": "^4.0.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.3",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
